[push]
	default = current
[core]
	autocrlf = input
	excludesfile = ~/.gitignore_global
	editor = emacs -nw
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	template = ~/.git_commit_msg.txt
