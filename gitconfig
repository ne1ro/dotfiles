[user]
	email = neiro.mail@gmail.com
	name = ne1ro
[push]
	default = current
[core]
	autocrlf = input
	excludesfile = $HOME/.gitignore_global
	editor = emacs
[credential]
	helper = osxkeychain
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	template = $HOME/.git_commit_msg.txt
